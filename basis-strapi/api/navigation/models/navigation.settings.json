{
  "kind": "collectionType",
  "collectionName": "navigations",
  "info": {
    "name": "Navigation"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "NavTitle": {
      "type": "string",
      "required": true
    },
    "NavURL": {
      "type": "string",
      "regex": "https?\\:\\/\\/",
      "required": true
    },
    "NavIcon": {
      "type": "string",
      "default": "$vuetify.icons.school",
      "required": true
    },
    "NavColor": {
      "type": "string",
      "default": "#ea6a20",
      "required": true
    }
  }
}
