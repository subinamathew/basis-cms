    },
    "URL": {
      "type": "string",
      "required": true,
      "regex": "https?\\:\\/\\/"
    },
    "Icon": {
      "type": "string",
      "default": "$vuetify.icons.school",
      "required": true
    },
    "Colour": {
      "type": "string",
      "default": "#ff5800",
      "required": true
    }
  }
}
